 //Return: array â€” without any duplicates

 const arr = [1, 1, 1, 1, 1, 1, 1 , 2 ,2 ,3, 4 ,5];

 const removeDup = (arr) => {
    let result = []
    let duplicate = []
    for (let i=0 ; i < arr.length; i++ ){
        if (arr[i] != arr[i+1]){
            result.push(arr[i])
        } else duplicate.push(arr[i])
    }
    return result;
 }

 console.log(removeDup(arr))


